<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>Omegalib</Name>
    <Version>@OMEGALIB_VERSION@</Version>
    <Title>Omegalib</Title>
    <Publisher>The Omegalib team</Publisher>
    <StartMenuDir>Omegalib</StartMenuDir>
    <TargetDir>@RootDir@omegalib</TargetDir>
    <ProductUrl>http://uic-evl.github.io/omegalib/</ProductUrl>
    <InstallerApplicationIcon>omega64-transparent</InstallerApplicationIcon>
    <InstallerWindowIcon>logo-shadow.png</InstallerWindowIcon>
    <Watermark>watermark.png</Watermark>
    <Banner>banner.png</Banner>
    <UninstallerName>PackageManager</UninstallerName>
    <RemoteRepositories>
     <Repository>
             <Url>http://omegalib.s3.amazonaws.com/repo/master</Url>
             <Enabled>1</Enabled>
             <DisplayName>Main repository</DisplayName>
     </Repository>
    </RemoteRepositories>
</Installer>